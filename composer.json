{
    "name": "Luminosity",
    "description": "A Lumen Test App.",
    "keywords": ["laravel", "lumen"],
    "license": "GPLv2",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "mockery/mockery": "^0.9.4",
        "phpmd/phpmd": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
